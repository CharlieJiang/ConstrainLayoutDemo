<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cocoas.constraintlayoutdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.cocoas.subpages.RelativePositioningActivity"></activity>
        <activity android:name="com.cocoas.subpages.MarginsActivity"></activity>
        <activity android:name="com.cocoas.subpages.CenteringPositioningActivity"></activity>
        <activity android:name="com.cocoas.subpages.CircularPositioningActivity"></activity>
        <activity android:name="com.cocoas.subpages.DimensionsConstraintsActivity"></activity>
        <activity android:name="com.cocoas.subpages.ChainsActivity"></activity>
        <activity android:name="com.cocoas.subpages.GuideLineActivity"></activity>
    </application>

</manifest>